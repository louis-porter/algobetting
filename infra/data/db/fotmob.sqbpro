umn index="8" value="65"/></column_widths><filter_values><column index="0" value="4813596"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="np_shots" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="69"/><column index="2" value="86"/><column index="3" value="54"/><column index="4" value="38"/><column index="5" value="32"/><column index="6" value="138"/><column index="7" value="80"/><column index="8" value="127"/><column index="9" value="164"/><column index="10" value="93"/><column index="11" value="65"/><column index="12" value="90"/></column_widths><filter_values><column index="0" value="4813596"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="team_id_mapping" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="61"/><column index="2" value="112"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">SELECT 
    teamId,
	b.team_name,
    SUM(expectedGoals) AS total_xG,
	SUM(expectedGoals) / 24 AS xG_per_90,
    COUNT(*) AS total_shots,
    AVG(expectedGoals) AS avg_xg_per_shot
FROM 
    np_shots a
JOIN team_id_mapping b ON a.teamId = b.team_id
WHERE
	league_id = 'Premier_League'
	AND season ='2025-2026'
GROUP BY 
    teamId
ORDER BY 
    total_xG DESC;</sql><sql name="SQL 2*">SELECT
	DISTINCT match_id
FROM 
	np_shots
WHERE
	season = '2025-2026' AND league_id = 'Premier_League' AND teamId = '9825'</sql><sql name="SQL 3*">        SELECT DISTINCT
            red.match_id,
            red.match_date,
            epv.team,
            epv.EPV,
            epv.season,
            epv.division as league_id
        FROM shots red
        JOIN team_id_mapping team ON team.team_id = red.teamId
        JOIN epv ON epv.team = team.team_name AND red.match_date = DATE(epv.startDate)                  
        WHERE
            division IN ('Premier_League')
            AND epv.season = '2025-2026'
			AND match_id = 4813596</sql><sql name="SQL 4*">        SELECT 
            match_id,
            match_date,
            league_id,
            season,
            team.team_name as team,
            side,
            expectedGoals,
            expectedGoalsOnTarget
        FROM np_shots fsd
            JOIN team_id_mapping team ON team.team_id = fsd.teamId
        WHERE
            league_id IN ('Premier_League')
            AND season = '2025-2026'
			AND match_id = 4813596</sql><sql name="SQL 5*">SELECT 
	AVG(home_goals) + (0.157 * 0.78),
	AVG(away_goals) + (0.101 * 0.78),
	AVG(home_goals) + (0.157 * 0.78) + AVG(away_goals) + (0.101 * 0.78) as total_goals
FROM
	np_matches
WHERE league_id = 'Premier_League'
AND season = '2025-2026';

SELECT 
	AVG(home_goals) 
	AVG(away_goals) ,
	AVG(home_goals) + AVG(away_goals) as total_goals
FROM
	np_matches
WHERE league_id = 'Premier_League'
AND season = '2025-2026'

</sql><sql name="SQL 6*">SELECT 
	AVG(home_goals) ,
	AVG(away_goals) ,
	AVG(home_goals) + AVG(away_goals) as total_goals
FROM
	np_matches
WHERE league_id = 'Premier_League'
AND season = '2025-2026'
</sql><sql name="SQL 7*">SELECT 
	AVG(home_goals) ,
	AVG(away_goals) ,
	AVG(home_goals) + AVG(away_goals) as total_goals
FROM
	matches
WHERE league_id = 'Premier_League'
AND season = '2025-2026'</sql><current_tab id="6"/></tab_sql></sqlb_project>
